{
	"name": "queue-backend",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.js",
	"engines": {
		"node": "^10"
	},
	"scripts": {
		"start": "node -r dotenv/config dist",
		"dev": "nodemon -r dotenv/config  --exec babel-node --presets=@babel/preset-env -- src/index.js",
		"build": "babel --delete-dir-on-start src/ --out-dir dist/ --presets=@babel/env",
		"format": "prettier --check --write \"!dist/*\" \"**/*.js\"",
		"lint": "eslint --fix src",
		"setup": "npm install && npm run lint && npm run format && cat .env.example > .env"
	},
	"keywords": [
		"queue",
		"fcrezza",
		"anang fachreza"
	],
	"author": {
		"name": "Anang Fachreza",
		"email": "fachrezanang@gmail.com"
	},
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^4.0.1",
		"cloudinary": "^1.25.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"express-mysql-session": "^2.1.4",
		"express-session": "^1.17.1",
		"express-socket.io-session": "^1.3.5",
		"joi": "^17.4.0",
		"morgan": "^1.10.0",
		"mysql2": "^2.2.5",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"socket.io": "^2.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.14",
		"@babel/core": "^7.13.15",
		"@babel/node": "^7.13.13",
		"@babel/preset-env": "^7.13.15",
		"@fcrezza/prettier-config": "^2.0.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-node": "^11.1.0",
		"husky": "^4.2.5",
		"nodemon": "^2.0.4",
		"prettier": "^2.0.5"
	},
	"prettier": "@fcrezza/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format"
		}
	}
}
